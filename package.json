{
    "name": "cargo-feature-hint",
    "displayName": "Cargo Feature Hint",
    "license": "MIT",
    "version": "1.0.2",
    "publisher": "Jerry457",
    "engines": {
        "vscode": "^1.100.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "repository": {
        "directory": "https://github.com/Jerry457/cargo-feature-hint",
        "type": "git"
    },
    "activationEvents": [
        "onLanguage:toml"
    ],
    "main": "./dist/extension.js",
    "contributes": {
        "languages": [
            {
                "id": "toml",
                "filenames": [
                    "Cargo.toml"
                ]
            }
        ]
    },
    "scripts": {
        "watch": "tsc --watch -p ./",
        "dev": "npm run build && npm run watch",
        "build": "tsc -p ./",
        "package": "npm i && npm run build && vsce package",
        "publish": "vsce publish"
    },
    "devDependencies": {
        "@types/node": "^25.0.9",
        "@types/vscode": "^1.100.0",
        "typescript": "~5.9.3"
    },
    "dependencies": {
        "axios": "^1.13.2"
    }
}
